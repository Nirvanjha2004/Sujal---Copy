    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "migrate": "ts-node src/utils/migration.ts up",
    "migrate:rollback": "ts-node src/utils/migration.ts rollback",
    "seed": "ts-node src/scripts/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
